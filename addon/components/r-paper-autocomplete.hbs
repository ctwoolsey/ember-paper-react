<span class={{this.componentType}} ...attributes
  {{ember-paper-reactable this.inserted this.changeReactState this.changeArgs}}
  {{did-update this.setValidation @errors}} {{did-update this.setValidation @isTouched}}
>
  {{#if (has-block 'groupHeaders')}}
    <AttributeNodeChild class="react=component {{this.groupHeaders}}" @attribute={{this.groupHeaders}} @loadAttributeInfo={{this.loadAttributeInfo}}>{{yield to='groupHeaders'}}</AttributeNodeChild>
  {{/if}}
  {{#if (has-block 'options')}}
    <AttributeNodeChild class="react=component {{this.options}}" @attribute={{this.options}} @loadAttributeInfo={{this.loadAttributeInfo}}>{{yield to='options'}}</AttributeNodeChild>
  {{/if}}
</span>
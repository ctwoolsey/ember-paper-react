<span class={{this.componentType}} ...attributes
  {{did-insert this.inserted }}
  {{did-update this.class @class}}
  {{did-update this.style @style}}
  {{did-update this.globalThemeChanged this.themeManager.theme}}
  {{did-update this.max @max}}
  {{did-update this.spacing @spacing}}
  {{did-update this.sx @sx}}
  {{did-update this.variant @variant}}
></span>
<span id={{this.childrenSpanId}} class="r-paper-avatar-group-children">
  {{yield}}
</span>
<div id={{this.lastChildId}}></div>

<span class={{this.componentType}} ...attributes
  {{ember-paper-reactable this.inserted this.changeReactState this.changeArgs}}
>
  {{#if (has-block 'avatar')}}
    <AttributeChild class="react=component avatar" @attribute="avatar" @loadAttributeInfo={{this.loadAttributeInfo}}>{{yield to='avatar'}}</AttributeChild>
  {{/if}}
  {{#if (has-block 'action')}}
    <AttributeChild class="react=component action" @attribute="action" @loadAttributeInfo={{this.loadAttributeInfo}}>{{yield to='action'}}</AttributeChild>
  {{/if}}
  {{#if (has-block 'subheader')}}
    <AttributeChild class="react=component subheader" @attribute="subheader" @loadAttributeInfo={{this.loadAttributeInfo}}>{{yield to='subheader'}}</AttributeChild>
  {{/if}}
  {{#if (has-block 'title')}}
    <AttributeChild class="react=component title" @attribute="title" @loadAttributeInfo={{this.loadAttributeInfo}}>{{yield to='title'}}</AttributeChild>
  {{/if}}
</span>